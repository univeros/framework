{
    "name": "univeros/cache",
    "description": "The Altair Cache package.",
    "license": "MIT",
    "homepage": "https://univeros.io",
    "support": {
        "issues": "https://github.com/univeros/framework/issues",
        "source": "https://github.com/univeros/framework"
    },
    "require": {
        "php": ">=7.0.0",
        "psr/cache": "^1.0",
        "psr/log": "~1.0",
        "univeros/configuration": "*",
        "univeros/filesystem": "*"
    },
    "autoload": {
        "psr-4": {
            "Altair\\Cache\\": ""
        }
