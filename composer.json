{
    "name": "univeros/framework",
    "description": "The Univeros Framework.",
    "keywords": ["framework", "univeros"],
    "license": "MIT",
    "homepage": "https://univeros.io",
    "support": {
        "issues": "https://github.com/univeros/framework/issues",
        "source": "https://github.com/univeros/framework"
    },
    "require": {
        "php": ">=7.0.0",
        "micheh/psr7-cache": "^0.5.0",
        "neomerx/cors-psr7": "^1.0",
        "nesbot/carbon": "~1.20",
        "nikic/fast-route": "^1.0",
        "psr/cache": "^1.0",
        "psr/http-message": "^1.0",
        "relay/middleware": "^1.0",
        "relay/relay": "^1.1",
        "vlucas/phpdotenv": "^2.4",
        "willdurand/negotiation": "^2.0",
        "zendframework/zend-diactoros": "^1.0.4",
        "league/flysystem": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.2",
        "squizlabs/php_codesniffer": "^2.7",
        "friendsofphp/php-cs-fixer": "^2.0",
        "phpmd/phpmd": "^2.4",
        "league/flysystem-aws-s3-v2": "~1.0",
        "league/flysystem-azure": "~1.0",
        "league/flysystem-cached-adapter": "~1.0",
        "league/flysystem-copy": "~1.0",
        "league/flysystem-dropbox": "~1.0",
        "league/flysystem-gridfs": "~1.0",
        "league/flysystem-rackspace": "~1.0",
        "league/flysystem-replicate-adapter": "~1.0",
        "league/flysystem-sftp": "~1.0",
        "league/flysystem-webdav": "~1.0",
        "league/flysystem-ziparchive": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "Altair\\": "src/Altair"
        }
    },
    "replace": {
        "univeros/base": "self.version",
        "univeros/configuration": "self.version",
        "univeros/container": "self.version",
        "univeros/http": "self.version",
        "univeros/queue": "self.version",
        "univeros/security": "self.version",
        "univeros/session": "self.version",
        "univeros/structure": "self.version"
    },
    "autoload-dev": {
        "psr-4": {
            "Altair\\Tests\\": "tests"
        },
        "files": [
            "tests/Container/fixtures.php"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "suggest": {
        "cache/cache": "To implement PSR-6 complain caching"
    },
    "minimum-stability": "dev"
}
